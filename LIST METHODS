s=["sriram","jai","ram","jai","jai","ram"]

s2=["hanuman"]

s.insert(2,"dasharth")

print(s)

print('*'*35)

s.append("sita")

print(s)

print('*'*35)

s.pop()

print(s)

print('*'*35)

s.reverse()

print(s)

print('*'*35)

s.count("jai")

print(s)

print('*'*35)

s1=s.copy()

