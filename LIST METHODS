# 7.sort()

s=["jai","shri","ram","jai"]

s.sort()

print(s)

['jai', 'jai', 'ram', 'shri']

# 8.remove()

s=["jai","shri","ram","jai"]

s.remove("jai")

print(s)

['shri', 'ram', 'jai']

# 9.clear()

s=["jai","shri","ram","jai"]

s.clear()

print(s)

[]

