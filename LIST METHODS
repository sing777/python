LIST METHODS

# insert()

s=["jai","shri","ram","jai"]

s.insert(4,"hanuman")

print(s)

s=["jai","shri","ram","jai"]

s.append("jai")

print(s)

['jai', 'shri', 'ram', 'jai', 'jai']

2# pop

​

s=["jai","shri","ram","jai"]

s.pop()

print(s)

['jai', 'shri', 'ram']

3.# reverse()

s=["jai","shri","ram","jai"]

s.reverse()

print(s)

['jai', 'ram', 'shri', 'jai']

4.# count()

s=["jai","shri","ram","jai"]

s1=s.count("jai")

print(s1)

2

# 5.copy()

s=["jai","shri","ram","jai"]

s1=s.copy()

print(s1)

['jai', 'shri', 'ram', 'jai']

# 6.extend()

s=["jai","shri","ram","jai"]

s1=["jai","shri","ram","jai"]

print(s.extend(s1))

s+s1

​

None

